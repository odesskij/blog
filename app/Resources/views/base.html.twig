<!DOCTYPE html>
<html>
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
  <title>{% block title -%}{%- endblock %}</title>
  <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
  {% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('bundles/framework/css/structure.css') }}" media="all"/>
    <link rel="stylesheet" href="{{ asset('bundles/framework/css/body.css') }}" media="all"/>
    <link rel="stylesheet" href="{{ asset('bundles/sensiodistribution/webconfigurator/css/install.css') }}" media="all"/>
  {% endblock %}
</head>
<body>
{% block body %}
  <div id="content">
    <div class="header clear-fix">
      <div class="header-logo">
        <img src="{{ asset('bundles/framework/images/logo_symfony.png') }}" alt="{{ 'Symfony' }}"/>
      </div>
    </div>
    <div class="sf-reset">
      <div class="block">
        <div class="symfony-block-content">
          <h2 class="title">{{ block('title') }}</h2>
          {% block content %}
            <p>Welcome to your new Symfony project.</p>
            <p>

            </p>
          {% endblock %}
        </div>
      </div>
    </div>
    <div class="version">{{ 'hello, world!'|upper }}</div>
  </div>
{% endblock %}
{% block javascripts %}
  <script type="text/javascript" src="{{ asset('resources/js/require.js') }}"></script>
  <script type="text/javascript" src="{{ asset('requirejs_config.js') }}"></script>
  <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
  <script src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>
{% endblock %}
</body>
</html>
